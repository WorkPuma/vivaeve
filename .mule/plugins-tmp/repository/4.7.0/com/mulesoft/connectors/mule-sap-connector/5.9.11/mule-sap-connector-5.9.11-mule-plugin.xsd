  <xs:element xmlns="http://www.mulesoft.org/schema/mule/sap" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="Document-listenerType" substitutionGroup="mule:abstract-message-source" name="document-listener">
    <xs:annotation>
      <xs:documentation>This source listens for incoming IDocs.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="Document-listenerType">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractMessageSourceType">
        <xs:sequence minOccurs="0" maxOccurs="1">
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-redelivery-policy"></xs:element>
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-byte-streaming-strategy"></xs:element>
          <xs:element minOccurs="0" maxOccurs="1" ref="mule:abstract-reconnection-strategy"></xs:element>
          <xs:element minOccurs="0" maxOccurs="1" name="i-doc-render-options">
            <xs:annotation>
              <xs:documentation>IDoc Rendering options</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:complexContent>
                <xs:extension xmlns:sap="http://www.mulesoft.org/schema/mule/sap" base="sap:com.mulesoft.connector.sap.api.IDocRenderParameterGroup"></xs:extension>
              </xs:complexContent>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute type="xs:string" use="optional" name="gatewayHost">
          <xs:annotation>
            <xs:documentation>This is the name of the host running the gateway handler. SAP Gateway is a development framework used to connect non-SAP applications to SAP applications. You can configure the SAP Gateway host to be the trusted system using an RFC destination.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="gatewayService">
          <xs:annotation>
            <xs:documentation>Either the name or service port for the gateway service of the SAP system</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="programID">
          <xs:annotation>
            <xs:documentation>SAP system program ID that is registered on the gateway. Must be unique for the given gateway.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:substitutableInt" use="optional" default="1" name="connectionCount">
          <xs:annotation>
            <xs:documentation>This parameter indicates the number of connections to register on the gateway. If there is more than one connection, handler requests are processed in parallel. For example, if two connections are set, this will create two handler workers or threads that are going to handle requests in parallel, almost like having two sources start together.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="idocTypeFilterRegex">
          <xs:annotation>
            <xs:documentation>Expression to filter all incoming IDocs by type.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="idocTypeExtensionFilterRegex">
          <xs:annotation>
            <xs:documentation>Expression to filter all incoming IDocs by type extension.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="host">
          <xs:annotation>
            <xs:documentation>Host of the message server</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="port">
          <xs:annotation>
            <xs:documentation>The message server name or port number</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="systemId">
          <xs:annotation>
            <xs:documentation>The system ID of the system the message server belongs to</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="group">
          <xs:annotation>
            <xs:documentation>The logon group defined in ABAP which is identifying a set of application servers</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="updateInterval">
          <xs:annotation>
            <xs:documentation>The update time how often the list of application servers is fetched from the message server</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="iDocRenderOptions">
          <xs:annotation>
            <xs:documentation>IDoc Rendering options</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="optional" name="encoding"></xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/sap" type="org.mule.sdk.api.tx.SourceTransactionalActionEnumType" use="optional" default="NONE" name="transactionalAction">
          <xs:annotation>
            <xs:documentation>The type of beginning action that sources can take regarding transactions.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:transactionType" use="optional" default="LOCAL" name="transactionType">
          <xs:annotation>
            <xs:documentation>The type of transaction to create. Availability will depend on the runtime version.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="mule:substitutableBoolean" use="optional" name="primaryNodeOnly">
          <xs:annotation>
            <xs:documentation>Whether this source should only be executed on the primary node when runnning in Cluster</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute type="xs:string" use="required" name="config-ref">
          <xs:annotation>
            <xs:documentation>The name of the configuration to be used to execute this component</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="com.mulesoft.connector.sap.api.IDocRenderParameterGroup">
    <xs:annotation>
      <xs:documentation>IDoc Rendering options</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractExtensionType">
        <xs:attribute type="mule:substitutableBoolean" use="optional" default="false" name="renderIDocEmptyTags"></xs:attribute>
        <xs:attribute type="mule:substitutableBoolean" use="optional" default="false" name="renderIDocIgnoreInvalidCharErrors"></xs:attribute>
        <xs:attribute type="mule:substitutableBoolean" use="optional" default="false" name="renderIDocIgnoreTagEscapeErrors"></xs:attribute>
